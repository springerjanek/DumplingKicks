---
import { ContactForm } from "../components/Forms/ContactForm";
import instagram from "../assets/instagram.svg";
import mail from "../assets/mail.svg";
import "../styles/styles.scss";
import "../styles/contact.scss";
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/png" href="https://i.imgur.com/ieEjw41.png" />
    <meta name="generator" content={Astro.generator} />
    <title>Dumpling Kicks</title>
  </head>
  <body>
    <div class="container">
      <ContactForm client:load />
      <div class="info">
        <div class="info__instagram">
          <img src={instagram} />
          <a href="https://www.instagram.com/dumpling_kicks"><p>Instagram</p></a
          >
        </div>
        <div class="info__mail">
          <img src={mail} />
          <p>dumplingkicks@gmail.com</p>
        </div>
      </div>
    </div>
  </body>
</html>
